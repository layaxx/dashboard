{
  "name": "dashboard",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "docker:build": "docker build . -t locally_built_dashboard_app",
    "docker:run:db": "docker compose -f docker-compose-db-only.yml up",
    "docker:run:all": "docker compose up -d"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "7.16.7",
    "@heroicons/react": "1.0.5",
    "@prisma/client": "3.15.1",
    "@reduxjs/toolkit": "1.7.2",
    "@tailwindcss/typography": "0.5.1",
    "autoprefixer": "10.x",
    "blitz": "0.45.2",
    "clsx": "1.1.1",
    "dayjs": "1.10.7",
    "final-form": "4.20.6",
    "htmlparser2": "7.2.0",
    "node-fetch": "3.2.3",
    "postcss": "8.x",
    "react": "18.0.0-beta-149b420f6-20211119",
    "react-dom": "18.0.0-alpha-5ca4b0433-20211020",
    "react-final-form": "6.5.7",
    "react-tracked": "1.7.6",
    "react-tsparticles": "1.41.0",
    "reapop": "4.0.3",
    "tailwindcss": "3.x",
    "ts-node": "10.7.0",
    "use-read-time": "1.0.2",
    "victory": "36.5.0",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.38",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-plugin-classnames": "https://github.com/layaxx/eslint-plugin-classnames",
    "eslint-plugin-tailwindcss": "3.4.3",
    "eslint-plugin-unicorn": "40.1.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.8.0",
    "pretty-quick": "3.1.3",
    "prisma": "3.15.1",
    "typescript": "~4.5"
  },
  "private": true
}
