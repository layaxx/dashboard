{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "jest.autoRun": "off",
  "cSpell.words": [
    "atalho",
    "blitzjs",
    "chartjs",
    "classname",
    "domutils",
    "Enriqueta",
    "Feedentries",
    "feedentry",
    "FEEDID",
    "feedoption",
    "feedoptions",
    "HEALTHCHECK",
    "heroicons",
    "Intervall",
    "LOCALSTORAGE",
    "notistack",
    "Readlist",
    "readlistentries",
    "readlistentry",
    "reapop",
    "sortablejs",
    "tailwindcss",
    "tsparticles",
    "unhide"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
