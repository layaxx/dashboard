{
  "name": "dashboard",
  "version": "1.0.0",
  "scripts": {
    "predev": "node scripts/writeVersion.js --dev",
    "dev": "blitz dev",
    "prebuild": "node scripts/writeVersion.js",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "docker:build": "docker build . -t locally_built_dashboard_app",
    "docker:run:db": "docker compose --env-file ./.env.local -f docker-compose-db-only.yml up",
    "docker:run:all": "docker compose --env-file ./.env.local up -d --build"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-beta.16",
    "@blitzjs/next": "2.0.0-beta.16",
    "@blitzjs/rpc": "2.0.0-beta.16",
    "@heroicons/react": "2.0.13",
    "@prisma/client": "4.5.0",
    "@tailwindcss/typography": "0.5.7",
    "blitz": "2.0.0-beta.16",
    "chart.js": "^3.0.0",
    "chartjs-adapter-dayjs-3": "1.2.3",
    "clsx": "1.2.1",
    "dayjs": "1.11.6",
    "feed-reader": "https://github.com/layaxx/feed-reader",
    "final-form": "4.20.7",
    "html-react-parser": "3.0.4",
    "next": "13.0.2",
    "node-fetch": "3.2.10",
    "react": "18.2.0",
    "react-chartjs-2": "4.3.1",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-sortablejs": "6.1.4",
    "react-toastify": "9.1.1",
    "react-tracked": "1.7.10",
    "react-tsparticles": "2.5.1",
    "sortablejs": "1.15.0",
    "tailwindcss": "3.2.2",
    "use-read-time": "1.0.2",
    "xss": "1.0.14",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "@testing-library/jest-dom": "5.16.5",
    "@types/babel__core": "7.1.19",
    "@types/jest": "28.1.3",
    "@types/react": "18.0.0",
    "@types/sortablejs": "1.15.0",
    "autoprefixer": "10.4.12",
    "eslint": "8.23.1",
    "eslint-config-blitz": "0.45.5",
    "eslint-plugin-classnames": "https://github.com/layaxx/eslint-plugin-classnames",
    "eslint-plugin-tailwindcss": "3.4.3",
    "eslint-plugin-unicorn": "40.1.0",
    "husky": "7.0.4",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "lint-staged": "11.3.0-beta.2",
    "node-mocks-http": "1.11.0",
    "postcss": "8.4.4",
    "postcss-nested": "5.0.6",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.8.0",
    "pretty-quick": "3.1.3",
    "prisma": "4.3.1",
    "ts-jest": "28.0.8",
    "typescript": "~4.5"
  },
  "private": true
}
