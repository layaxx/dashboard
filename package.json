{
  "name": "dashboard",
  "version": "1.0.0",
  "scripts": {
    "predev": "node scripts/writeVersion.js --dev",
    "dev": "blitz dev",
    "prebuild": "node scripts/writeVersion.js",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "docker:build": "docker build . -t locally_built_dashboard_app",
    "docker:run:db": "docker compose --env-file ./.env.local -f docker-compose-db-only.yml up",
    "docker:run:all": "docker compose --env-file ./.env.local up -d --build"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@heroicons/react": "1.0.5",
    "@prisma/client": "4.0.0",
    "@tailwindcss/typography": "0.5.1",
    "blitz": "0.45.4",
    "chart.js": "^3.0.0",
    "chartjs-adapter-dayjs-3": "1.2.3",
    "clsx": "1.1.1",
    "dayjs": "1.10.7",
    "feed-reader": "https://github.com/layaxx/feed-reader",
    "final-form": "4.20.6",
    "html-react-parser": "3.0.0",
    "node-fetch": "3.2.3",
    "react": "18.0.0-beta-149b420f6-20211119",
    "react-chartjs-2": "4.2.0",
    "react-dom": "18.0.0-alpha-5ca4b0433-20211020",
    "react-final-form": "6.5.7",
    "react-sortablejs": "6.1.4",
    "react-tracked": "1.7.9",
    "react-tsparticles": "1.41.0",
    "reapop": "4.0.3",
    "sortablejs": "1.15.0",
    "tailwindcss": "3.x",
    "use-read-time": "1.0.2",
    "xss": "1.0.13",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@faker-js/faker": "7.3.0",
    "@types/react": "18.0.0",
    "@types/sortablejs": "1.13.0",
    "autoprefixer": "10.4.7",
    "eslint": "7.32.0",
    "eslint-plugin-classnames": "https://github.com/layaxx/eslint-plugin-classnames",
    "eslint-plugin-tailwindcss": "3.4.3",
    "eslint-plugin-unicorn": "40.1.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "node-mocks-http": "1.11.0",
    "postcss": "8.4.4",
    "postcss-nested": "5.0.6",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.8.0",
    "pretty-quick": "3.1.3",
    "prisma": "4.0.0",
    "typescript": "~4.5"
  },
  "private": true
}
