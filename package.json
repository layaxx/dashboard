{
  "name": "dashboard",
  "version": "1.0.0",
  "scripts": {
    "predev": "node scripts/writeVersion.js --dev",
    "dev": "blitz dev",
    "prebuild": "node scripts/writeVersion.js",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "tsc && eslint --ignore-path .gitignore --ext .js,.ts,.tsx . && blitz prisma format --check",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "docker:build": "docker build . -t locally_built_dashboard_app",
    "docker:run:db": "docker compose --env-file ./.env.local -f docker-compose-db-only.yml up",
    "docker:run:all": "docker compose --env-file ./.env.local up -d --build"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.1.3",
    "@blitzjs/next": "2.1.3",
    "@blitzjs/rpc": "2.1.3",
    "@heroicons/react": "2.0.12",
    "@prisma/client": "^6.1.0",
    "@tailwindcss/typography": "0.5.1",
    "blitz": "2.1.3",
    "chart.js": "^3.0.0",
    "chartjs-adapter-dayjs-3": "1.2.3",
    "clsx": "2.1.1",
    "dayjs": "1.11.11",
    "feed-reader": "https://github.com/layaxx/feed-reader",
    "final-form": "4.20.10",
    "he": "1.2.0",
    "html-react-parser": "5.1.10",
    "next": "^13.5.6",
    "node-fetch": "3.3.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-confirm": "0.3.0-7",
    "react-dom": "^18.2.0",
    "react-final-form": "6.5.9",
    "react-sortablejs": "6.1.4",
    "react-toastify": "10.0.5",
    "react-tracked": "^2.0.0",
    "react-tsparticles": "1.41.0",
    "secure-password": "4.0.0",
    "sortablejs": "1.15.2",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "^3.4.0",
    "univis-api": "0.0.3",
    "use-read-time": "1.0.3",
    "xss": "1.0.15",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@testing-library/jest-dom": "5.16.5",
    "@types/babel__core": "7.1.19",
    "@types/he": "1.2.3",
    "@types/jest": "28.1.3",
    "@types/react": "18.0.0",
    "@types/sortablejs": "1.15.0",
    "autoprefixer": "10.4.12",
    "eslint": "8.23.1",
    "eslint-config-blitz": "0.45.5",
    "eslint-plugin-classnames": "https://github.com/layaxx/eslint-plugin-classnames",
    "eslint-plugin-tailwindcss": "3.15.1",
    "eslint-plugin-unicorn": "40.1.0",
    "husky": "9.0.11",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-mock-extended": "2.0.4",
    "lint-staged": "11.3.0-beta.2",
    "node-mocks-http": "1.14.1",
    "postcss": "8.4.38",
    "postcss-nested": "5.0.6",
    "prettier": "3.3.0",
    "prettier-plugin-prisma": "5.0.0",
    "pretty-quick": "^4.0.0",
    "prisma": "^6.1.0",
    "ts-jest": "28.0.8",
    "typescript": "~4.7"
  },
  "private": true
}
